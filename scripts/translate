SCRIPT
to arcr :r :deg
	repeat :deg [
		forward :r
		right 1
	]
end


to arcl :r :deg
	repeat :deg [
		forward :r
		left 1
	]
end

to ray :r
	repeat 2 [
		arcl :r 90
		arcr :r 90
	]
end

to sun :size
	repeat 9 [
		ray :size
		right 160
	]
end

to main
	pu
	rt 180
	fd 100
	lt 90
	fd 100
	rt lt 90
	pd
	sun 0.5
	translate (-100) (-100)
	fd 1
end

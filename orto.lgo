SCRIPT
to ortocircle :side :invrad :len
	inverse 0 0 :invrad
	psu
	fd :invrad
	psd
	make "path 0
	fd :side
	while :path < :len
	[
		while getx * getx + gety * gety > :invrad * :invrad 
		[
			rt 1
			fd :side
			make "path :path + 1
		]
		toggle
	]
end

to psu 
	pu toggle pu toggle
end

to psd
	pd toggle pd toggle
end
to main
	ortocircle 1 100 1300
end

